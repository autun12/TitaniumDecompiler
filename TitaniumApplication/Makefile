GENERATED :=
OBJECTS :=

GENERATED += $(OBJDIR)/HexViewerPanel.o
GENERATED += $(OBJDIR)/SectionsPanel.o
GENERATED += $(OBJDIR)/TitaniumApp.o
GENERATED += $(OBJDIR)/TitaniumLayer.o
